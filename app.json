{
  "expo": {
    "name": "ThinkRealty",
    "slug": "ThinkRealty",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/home.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.thinkrealty.ae",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.thinkrealty.ae",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-notifications",
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "3a0b8c8e-091d-4876-adc0-58bdce555fd4"
      }
    }
  }
}
